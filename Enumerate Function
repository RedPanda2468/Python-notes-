#Enumerate function
'''

marks= [12,56,32,98,12,45,1,4]
index= 0
for mark in marks:
  print(mark)

  if (index==3):
     print("you are awesome")
  index=index+1
  '''


  #OR 
'''
for index, mark in enumerate(marks):
  print(mark)
  if (index==3):
    print("you are awesome")

for index, mark in enumerate(marks,start=1):
   print(mark)
   if (index==3):
    print("awesome!")
    '''
